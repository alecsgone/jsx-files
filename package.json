{
  "name": "jsx-render",
  "version": "2.0.1",
  "description": "",
  "main": "element.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alecsgone/jsx-render.git"
  },
  "keywords": [
    "JSX",
    "jsx-render",
    "SVG-JSX"
  ],
  "author": "Aleks",
  "bugs": {
    "url": "https://github.com/alecsgone/jsx-render/issues"
  },
  "homepage": "https://github.com/alecsgone/jsx-render#readme",
  "license": "MIT",
  "scripts": {
    "ssrbuildclient": "webpack --config examples/ssr/webpack.config.js",
    "ssrclient": "ONLY_CLIENT=true webpack-dev-server --config examples/ssr/webpack.config.js",
    "ssr": "npm run ssrbuildclient & babel-node --config-file ./.babelrc examples/ssr/server.js",
    "start": "webpack-dev-server --config examples/webpack.config.js --hot",
    "dev": "babel src --out-dir lib --watch",
    "build": "babel src --out-dir ./",
    "eslint": "eslint src/.",
    "precommit": "pretty-quick --staged",
    "prepush": "npm run test",
    "prepare": "npm run test && npm run build",
    "prepublish": "npm run test && npm run build",
    "posttest": "npm run eslint",
    "test": "ava test/*.* --verbose"
  },
  "ava": {
    "require": [
      "@babel/register",
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/register": "^7.0.0",
    "ava": "^1.0.1",
    "babel-loader": "^8.0.0-beta.3",
    "babel-preset-primavera": "^1.1.0",
    "body-parser": "^1.18.3",
    "browser-env": "^3.2.5",
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "express": "^4.16.4",
    "global": "^4.3.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.1",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.7.0",
    "redux": "^4.0.0",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "optionalDependencies": {
    "cheerio": "^1.0.0-rc.2"
  }
}
